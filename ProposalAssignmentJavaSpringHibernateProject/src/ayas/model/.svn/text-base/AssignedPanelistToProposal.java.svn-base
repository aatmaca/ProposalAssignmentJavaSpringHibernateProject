package ayas.model;

import java.io.Serializable;

/**
 *
 * @author abdullah
 */
public class AssignedPanelistToProposal implements Serializable{

   private AssignedPanelistToProposalId panelistToProposalId;
   private Integer grade;
    private String status;

    public AssignedPanelistToProposal() {
    }

    public AssignedPanelistToProposal(Integer recordId, Integer assignmentId) {
        this.panelistToProposalId = new AssignedPanelistToProposalId(recordId, assignmentId);
    }

    /**
     * @return the panelistToProposalId
     */
    public AssignedPanelistToProposalId getPanelistToProposalId() {
        return panelistToProposalId;
    }

    /**
     * @param panelistToProposalId the panelistToProposalId to set
     */
    public void setPanelistToProposalId(AssignedPanelistToProposalId panelistToProposalId) {
        this.panelistToProposalId = panelistToProposalId;
    }

    /**
     * @return the grade
     */
    public Integer getGrade() {
        return grade;
    }

    /**
     * @param grade the grade to set
     */
    public void setGrade(Integer grade) {
        this.grade = grade;
    }

    /**
     * @return the status
     */
    public String getStatus() {
        return status;
    }

    /**
     * @param status the status to set
     */
    public void setStatus(String status) {
        this.status = status;
    }
}
