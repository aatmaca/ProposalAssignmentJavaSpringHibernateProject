package ayas.model;

import java.io.Serializable;

/**
 *
 * @author abdullah
 */
public class Panelist implements Serializable{

    private Integer id;
    private Integer title_id;
    private Integer subject_id;
    private Integer company_id;
    private String name;
    private String surname;
    public static final Integer maxCapacity = new Integer(30);

    public Panelist() {
    }

    /**
     * @return the id
     */
    public Integer getId() {
        return id;
    }

    /**
     * @param id the id to set
     */
    public void setId(Integer id) {
        this.id = id;
    }

    /**
     * @return the name
     */
    public String getName() {
        return name;
    }

    /**
     * @param name the name to set
     */
    public void setName(String name) {
        this.name = name;
    }

    /**
     * @return the surname
     */
    public String getSurname() {
        return surname;
    }

    /**
     * @param surname the surname to set
     */
    public void setSurname(String surname) {
        this.surname = surname;
    }

    public Integer getTitle_id() {
        return title_id;
    }

    /**
     * @param surname the surname to set
     */
    public void setTitle_id(Integer t) {
        this.title_id = t;
    }

    public Integer getCompany_id() {
        return company_id;
    }

    /**
     * @param surname the surname to set
     */
    public void setCompany_id(Integer t) {
        this.company_id = t;
    }

    public Integer getSubject_id() {
        return subject_id;
    }

    /**
     * @param surname the surname to set
     */
    public void setSubject_id(Integer t) {
        this.subject_id = t;
    }

    public String getFullName() {
        return name + " " +surname;
    }
}
