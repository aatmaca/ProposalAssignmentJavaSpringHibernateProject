package ayas.command;

import ayas.model.Company;
import ayas.model.Subject;
import ayas.model.Title;

import java.io.Serializable;
import java.util.List;

/**
 *
 * @author asil
 */
public class RefereeSearchCommand implements Serializable{

    private String name;
    private String surname;
    private List<String> names;
    private List<String> surnames;
    private List<Company> companies;
    private List<Title> titles;
    private List<Subject> subjects;
    private Integer selectedCompany;
    private Integer selectedTitle;
    private Integer selectedSubject;
    
    private Integer selectedAssignmentID;


    public List<String> getNames() {
        return names;
    }


    public void setNames(List<String> t) {
        this.names = t;
    }

    public List<String> getSurnames() {
        return surnames;
    }


    public void setSurnames(List<String> t) {
        this.surnames = t;
    }

    public List<Company> getCompanies() {
        return companies;
    }


    public void setCompanies(List<Company> t) {
        this.companies = t;
    }

    public List<Title> getTitles() {
        return titles;
    }


    public void setTitles(List<Title> t) {
        this.titles = t;
    }

    public List<Subject> getSubjects() {
        return subjects;
    }


    public void setSubjects(List<Subject> t) {
        this.subjects = t;
    }

    /**
     * @return the selectedSubject
     */
    public Integer getSelectedSubject() {
        return selectedSubject;
    }

    /**
     * @param selectedSubject the selectedSubject to set
     */
    public void setSelectedSubject(Integer selectedSubject) {
        this.selectedSubject = selectedSubject;
    }

    /**
     * @return the name
     */
    public String getName() {
        return name;
    }

    /**
     * @param name the name to set
     */
    public void setName(String name) {
        this.name = name;
    }

    /**
     * @return the surname
     */
    public String getSurname() {
        return surname;
    }

    /**
     * @param surname the surname to set
     */
    public void setSurname(String surname) {
        this.surname = surname;
    }

    /**
     * @return the selectedCompany
     */
    public Integer getSelectedCompany() {
        return selectedCompany;
    }

    /**
     * @param selectedCompany the selectedCompany to set
     */
    public void setSelectedCompany(Integer selectedCompany) {
        this.selectedCompany = selectedCompany;
    }

    /**
     * @return the selectedTitle
     */
    public Integer getSelectedTitle() {
        return selectedTitle;
    }

    /**
     * @param selectedTitle the selectedTitle to set
     */
    public void setSelectedTitle(Integer selectedTitle) {
        this.selectedTitle = selectedTitle;
    }

    /**
     * @return the selectedAssignmentID
     */
    public Integer getSelectedAssignmentID() {
        return selectedAssignmentID;
    }

    /**
     * @param selectedAssignmentID the selectedAssignmentID to set
     */
    public void setSelectedAssignmentID(Integer selectedAssignmentID) {
        this.selectedAssignmentID = selectedAssignmentID;
    }

}
