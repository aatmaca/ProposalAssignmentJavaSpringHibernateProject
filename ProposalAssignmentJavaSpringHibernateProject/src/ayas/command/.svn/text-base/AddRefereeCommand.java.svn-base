public class AddRefereeCommand {
    private PanelCommand panelCommand;
    private int newRefereeCount;

    public PanelCommand getPanelCommand() {
        return panelCommand;
    }

    public void setPanelCommand(PanelCommand panelCommand) {
        this.panelCommand = panelCommand;
    }
    
    public int getNewRefereeCount(){
        return newRefereeCount;
    }
    
    public void setNewRefereeCount(int newRefereeCount){
        this.newRefereeCount = newRefereeCount;
    }
}