package ayas.command;

import java.io.Serializable;

/**
 *
 * @author hardemr
 */
public class PanelConfigurationCommand implements Serializable{
    
    private int refereeCount;
    private int proposalCount;
    private String panelName;
    

    public int getRefereeCount() {
        return refereeCount;
    }

    public void setRefereeCount(int refereeCount) {
        this.refereeCount = refereeCount;
    }

    public int getProposalCount() {
        return proposalCount;
    }

    public void setProposalCount(int proposalCount) {
        this.proposalCount = proposalCount;
    }

    /**
     * @return the panelName
     */
    public String getPanelName() {
        return panelName;
    }

    /**
     * @param panelName the panelName to set
     */
    public void setPanelName(String panelName) {
        this.panelName = panelName;
    }
    

}
